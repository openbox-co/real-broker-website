{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@public/*": [
        "public/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}